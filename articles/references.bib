	shortjournal = {{PNAS}},
	author = {Thurner, Stefan and Klimek, Peter and Hanel, Rudolf},
	urldate = {2021-05-18},
	date = {2020-09-15},
	langid = {english},
	pmid = {32839315},
	note = {Publisher: National Academy of Sciences
Section: Physical Sciences},
	keywords = {{COVID}-19, compartmental epidemiological model, mean-field (well mixed) approximation, network theory, social contact networks},
	file = {Full Text PDF:/Users/lassehyldighansen/Zotero/storage/5QFVI7RZ/Thurner et al. - 2020 - A network-based explanation of why most COVID-19 i.pdf:application/pdf;Snapshot:/Users/lassehyldighansen/Zotero/storage/SZNXSM7A/22684.html:text/html},
