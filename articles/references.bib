	langid = {english},
	note = {{ISSN}: 2640-3498},
	file = {Full Text PDF:/Users/lassehyldighansen/Zotero/storage/BWI8EHKL/Balestriero and baraniuk - 2018 - A Spline Theory of Deep Learning.pdf:application/pdf;Supplementary PDF:/Users/lassehyldighansen/Zotero/storage/8J4J3DW8/Balestriero and baraniuk - 2018 - A Spline Theory of Deep Learning.pdf:application/pdf},
}

