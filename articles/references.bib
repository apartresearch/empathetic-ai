
@article{adair_social_1991,
	title = {Social cognition, artifact and the passing of the so-called crisis in social psychology},
	volume = {32},
	issn = {1878-7304(Electronic),0708-5591(Print)},
	doi = {10.1037/h0079016},
	abstract = {Examines the relationship of the crisis of confidence in social psychology of the 1970s with the cognitive model ({CM}) that emerged. The {CM} refers to the study of how individuals cognitively process social stimuli and the relation between the cognitions of individuals and sociocultural representations. Ethical considerations may have been involved in the shift to the {CM}. Because artifact challenges focused attention of Ss' thoughts and perceptions, the crisis literature directly promoted the {CM}. Artifact research has also shaped the direction of research with the {CM}. Results indicate the need for greater attention to instructions to Ss; continued improved techniques for accessing Ss' cognitions; increased sensitivity to Ss' phenomenological experiences as research participants; and a broadening of a narrow methodological and substantive focus. ({PsycInfo} Database Record (c) 2020 {APA}, all rights reserved)},
